{
    "version": 2,
    "builds": [
        {
            "src": "Kalc/backend/server.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
          "src": "/api/(.*)", 
          "dest": "/api/" 
        },
        {
          "src": "/(.*)", 
          "dest": "/" 
        }
      ]
}